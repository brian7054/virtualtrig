generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Message {
  id        String   @id @default(cuid())
  name      String
  email     String
  body      String
  createdAt DateTime @default(now())
  read      Boolean  @default(false)
}
